{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Tauress",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true
      },
      "fs": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "publisher": "Sandun Wiratunga",
      "copyright": "Sandun Wiratunga",
      "category": "",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "Tauress",
      "longDescription": "Tauri App for Express Server Demo",
      "shortDescription": "Tauri App for Express Server Demo",
      "externalBin": [],
      "resources": [],
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "en-US"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        
      ],
      "windows": {
        "installMode": "passive"
      },
      "pubkey": ""
    },
    "windows": [
      {
        "title": "Tauress",
        "label": "main",
        "url": "https://tauressserver.sandunwira.repl.co",
        "fullscreen": false,
        "focus": true,
        "resizable": true,
        "decorations": true,
        "center": true
      }
    ]
  }
}
